ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/AudioPlayerSwift" "$PODS_CONFIGURATION_BUILD_DIR/DragDropUI" "$PODS_CONFIGURATION_BUILD_DIR/KDEAudioPlayer" "$PODS_CONFIGURATION_BUILD_DIR/LNICoverFlowLayout" "$PODS_CONFIGURATION_BUILD_DIR/NVActivityIndicatorView" "$PODS_CONFIGURATION_BUILD_DIR/SwiftySound"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/AudioPlayerSwift/AudioPlayer.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/DragDropUI/DragDropUI.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/KDEAudioPlayer/KDEAudioPlayer.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/LNICoverFlowLayout/LNICoverFlowLayout.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/NVActivityIndicatorView/NVActivityIndicatorView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SwiftySound/SwiftySound.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "AudioPlayer" -framework "DragDropUI" -framework "KDEAudioPlayer" -framework "LNICoverFlowLayout" -framework "NVActivityIndicatorView" -framework "SwiftySound"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
